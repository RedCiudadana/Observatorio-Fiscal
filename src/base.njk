			<div id="header-wrap" >
				<div class="container">				
					<div class="header-row justify-content-lg-between">

						<!-- Logo
						============================================= -->
						<div id="logo">
							<a href="/index.html" class="standard-logo"><img style="padding:5%" src="/elementos/LOGOTIPO/LOGOTIPO-NEGRO.png" alt="Canvas Logo"></a>
							<a href="/index.html" class="retina-logo"><img style="padding:5%" src="/elementos/LOGOTIPO/LOGOTIPO-NEGRO.png" alt="Canvas Logo"></a>
						</div><!-- #logo end -->


						<!-- Primary Navigation
						============================================= -->
						<nav class="primary-menu">


							<ul class="menu-container">
								<li class="menu-item "><a class="menu-link" href="/gobcentral/index.html"><div>Gobierno Central</div></a></li>
								<li class="menu-item "><a class="menu-link" href="/municipal/index.html"><div>Municipal</div></a></li>
								<li class="menu-item"><a class="menu-link" href="/obraspublicas/index.html"><div>Obras Públicas</div></a></li>
								<li class="menu-item"><a class="menu-link" href="/contratacionespublicas/index.html"><div>Contrataciones Públicas</div></a></li>
								<li class="menu-item"><a class="menu-link" href="#"><div></div>Blog</a></li>
								<li class="menu-item"><a class="menu-link" href="#"><div></div>Eventos</a></li>
							</ul>

						</nav><!-- #primary-menu end -->

					</div>
				</div>
			</div>
			<div class="header-wrap-clone"></div>
		</header><!-- #header end -->

		{% block content %}
        {% endblock %} 

